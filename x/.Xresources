! {{{ Main

URxvt.cursorUnderline:     1
URxvt.fading:              1
URxvt.cursorBlink:         false
URxvt.imLocale:            en_US.UTF-8
URxvt.scrollBar:           false
URxvt.internalBorder:      10
URxvt.visualBell:          false
URxvt.lineSpace:           1.4
URxvt.letterSpace:         0
URxvt.url-launcher:        /usr/local/bin/firefox
URxvt.buffered:            false

URxvt.dpi:                 96
URxvt.antialias:           true
URxvt.hinting:             true
URxvt.rgba:                rgb
URxvt.autohint:            true
URxvt.hintstyle:           hintslight
URxvt.lcdfilter:           lcddefault
URxvt.depth:               24

Xcursor.size:              12

xterm*cursorUnderline:     true
xterm*scaleHeight:         1.04

! }}}
